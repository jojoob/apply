<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="enrol/apply/db" VERSION="2014061000" COMMENT="XMLDB file for Moodle enrol apply method"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
        >
    <TABLES>
        <TABLE NAME="enrol_apply" COMMENT="Holds all known information about apply method">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="enrolid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="q1" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
                <FIELD NAME="q2" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
